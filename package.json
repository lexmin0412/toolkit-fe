{
  "name": "toolkit-frontend",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm --filter=@toolkit-fe/* run build",
    "ci:publish": "pnpm --filter=@toolkit-fe/* publish",
    "clear": "rimraf 'packages/*/{lib,es,node_modules}' && rimraf node_modules",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build && pnpm release:only",
    "release:only": "changeset publish --registry=https://registry.npmjs.com/"
  },
  "engines": {
    "node": ">=18.0.0 <19.0.0",
    "pnpm": ">=8.0.0 <9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lexmin0412/toolkit-js.git"
  },
  "keywords": [],
  "author": "lexmin0412 <zhangle_dev@outlook.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lexmin0412/toolkit-js/issues"
  },
  "homepage": "https://github.com/lexmin0412/toolkit-js#readme",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "only-allow": "^1.2.1"
  },
  "workspaces": [
    "packages/*"
  ]
}
