  },
  "homepage": "https://github.com/lexmin0412/toolkit-fe#readme",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "lerna": "^8.0.0",
    "only-allow": "^1.2.1"
  },
  "workspaces": [
    "packages/*"
